@inproceedings{Bendersky:2008:DKC:1390334.1390419,
	author = {Bendersky, Michael and Croft, W. Bruce},
 title = {Discovering Key Concepts in Verbose Queries},
 booktitle = {Proceedings of the 31st Annual International ACM SIGIR Conference on Research and Development in Information Retrieval},
 series = {SIGIR '08},
 year = {2008},
 isbn = {978-1-60558-164-4},
 location = {Singapore, Singapore},
 pages = {491--498},
 numpages = {8},
 url = {http://doi.acm.org/10.1145/1390334.1390419},
 doi = {10.1145/1390334.1390419},
 acmid = {1390419},
 publisher = {ACM},
 address = {New York, NY, USA},
 keywords = {information retrieval, key concepts extraction, verbose queries},
	annotate = {This paper outlines an approach to automatically extract key concepts from verbose search queries. The authors claim that their method achieves higher accuracy in concept identification than standard weighting methods such as inverse document frequency. One issue discussed regarding verbose queries is that since they likely have numerous key concepts, many search results with inevitably be documents that are relevant to a subset of these key concepts. Additionally, some results may be documents that make reference to more than one concept, however do not match the query in terms of the relation between said concepts. This is said to create the paradoxical situation where less descriptive queries achieve better retrieval effectiveness. \\
	
A ranking principle is used to rank a document in response to a given q
uery by estimating the probability P(q|d). The paper then goes into tec
hnical detail about how to infer this by calculating probabilities of c
oncepts being in the presence of the doc/query etc.

The authors considered noun phrases to be "concepts", which have suppod
ely been proven to be reliable for key concept discovery in existing wo
rk. 

A concept weighting phase is decribed, whereby the probability of a con
cept existing, given a query, to essentially reflect how well a concept
 represents the query at hand. A supervised machine learning approach i
s taken in order to predict which concepts might be the best representa
tions of the query. The authors proceed to go into technical detail reg
arding their approach and the various features used.
\\	
	
A limitation of this method is that it is a strict reduction of the existing term set (does not support novel term inference). Additionally, the authors used topics 12–49 terms in length, while our patient reports are 39–204 terms in length. The significant differences in verbosity may be an issue.}	
}

@inproceedings{Soldaini2016QuickumlsAF,
	Author = {Luca Soldaini and Nazli Goharian},
	Booktitle = {MedIR Workshop, SIGIR},
	Date-Added = {2017-02-20 11:41:55 +0000},
	Date-Modified = {2017-02-20 11:49:50 +0000},
	Title = {Quickumls: a Fast, Unsupervised Approach for Medical Concept Extraction},
	Year = {2016},
	annotate = {This work seeks to present a medical concept extraction algorithm that scales with speed to large datasets. It is suggested that previous medical concept extraction systems focus primarily on the improvement of precision and recall with little attention being made to managing the efficiency of their computation; which, in turn, limits their suitability for large datasets. The proposed system (QuickUMLS) uses an approximate dictionary matching algorithm in order to reduce the amount of computation required. Results show that the proposed system achieves similar recall and precision to that of state-of-the-art systems but with significantly improved efficiency (up to 135 times faster than both systems compared against). The QuickUMLS system was used for the medical concept extraction required in the previous part of the project. The results produced by the query reduction model which utilised this system were of a high level.}}

@inproceedings{Soldaini2015RetrievingML,
	author="Soldaini, Luca
and Cohan, Arman
and Yates, Andrew
and Goharian, Nazli
and Frieder, Ophir",
title="Retrieving Medical Literature for Clinical Decision Support",
bookTitle="Advances in Information Retrieval: 37th European Conference on IR Research, ECIR 2015, Vienna, Austria, March 29 - April 2, 2015. Proceedings",
year="2015",
publisher="Springer International Publishing",
address="Cham",
pages="538--549",
abstract="Keeping current given the vast volume of medical literature published yearly poses a serious challenge for medical professionals. Thus, interest in systems that aid physicians in making clinical decisions is intensifying. A task of Clinical Decision Support (CDS) systems is retrieving highly relevant medical literature that could help healthcare professionals in formulating diagnoses or determining treatments. This search task is atypical as the queries are medical case reports, which differs in terms of size and structure from queries in other, more common search tasks. We apply query reformulation techniques to address literature search based on case reports. The proposed system achieves a statistically significant improvement over the baseline (29{\%} -- 32{\%}) and the state-of-the-art (12{\%} -- 59{\%}).",
isbn="978-3-319-16354-3",
doi="10.1007/978-3-319-16354-3_59",
url="https://doi.org/10.1007/978-3-319-16354-3_59"
	annotate = {Soldaini et al. experimented with query reduction techniques for
searching medical literature. The scope of the paper is based around the implementation of a CDS system designed for the retrieval of key medical literature that can assist the practice of healthcare professionals given a medical case report. Mention is made to the fact that  CDS search highly favors precision over recall, since healthcare professionals can only afford to spend limited time reading medical literature while practicing. \\

	The paper explores numerous query reduction and expansion techniques. Reduction techniques include: UMLS concept selection; health-related term selection (powered with wikipedia dump); and Query Quality Predictors (\textit{based on QPP techniques proposed by Kumaran and Carvalho}). The results for the UMLS based reduction were rather poor, which was attributed to the fact that it often discards terms that have a key role connecting domain specific expression (despite it successfully identifying most medical concepts). Their QQP method also performed quite poorly, which was seen to be a result of its aggresive reduction algorithm (reduces report to 6 terms at most). \\

	This paper is highly relevant to our project insofar as it pertains to information retreival where a verbose patient report is used as the query. Although our UMLS reduction proved quite effective, taking note of the concerns expressed in this paper could provide us with some insight from which we can improve our reduction model. Perhaps if we developed some method by which we could retain important terms that connect domain specific expressions we could see enhanced results. 
}}

@Article{Roberts2016,
author="Roberts, Kirk
and Simpson, Matthew
and Demner-Fushman, Dina
and Voorhees, Ellen
and Hersh, William",
title="State-of-the-art in biomedical literature retrieval for clinical cases: a survey of the TREC 2014 CDS track",
journal="Information Retrieval Journal",
year="2016",
volume="19",
number="1",
pages="113--148",
abstract="Providing access to relevant biomedical literature in a clinical setting has the potential to bridge a critical gap in evidence-based medicine. Here, our goal is specifically to provide relevant articles to clinicians to improve their decision-making in diagnosing, treating, and testing patients. To this end, the TREC 2014 Clinical Decision Support Track evaluated a system's ability to retrieve relevant articles in one of three categories (Diagnosis, Treatment, Test) using an idealized form of a patient medical record. Over 100 submissions from over 25 participants were evaluated on 30 topics, resulting in over 37k relevance judgments. In this article, we provide an overview of the task, a survey of the information retrieval methods employed by the participants, an analysis of the results, and a discussion on the future directions for this challenging yet important task.",
issn="1573-7659",
doi="10.1007/s10791-015-9259-x",
url="http://dx.doi.org/10.1007/s10791-015-9259-x",
annotate = {This survey provides an analysis of the TREC 2014 Clinical Decision Support track and the different approaches taken by its participants. This survey is highly relevant to my research as the task in question involved using the same collection of patient reports as query objects. The analysis of each of the participants' approaches provides me with a collated overview of which query modification techniques were successful. By examing these approaches I can improve my own reduction strategies without performing extensive tests of my own. \\

\textbf{Concept Extraction}: \\
 \\

 \textbf{Negation}: \\
Some particpants utilised negation detection techniques. Variations of these include the pruning of negated concepts, adjusting weight to prefer polarity-matched concepts, as well as replacing negated concepts with an altered representation.  \\
 
  \textbf{Attribute Extraction}: \\
Some participants tried to extract patient attributes that could be deemed clinically important from the reports. Such attributes include a patient's age, gender, and race. Soldaini et al adjusted article scores based on attribute matches, which resulted in their best run. It would be worthwhile to investigate these techniques further as they could form a part of future versions of my reduction model.
  }
}

